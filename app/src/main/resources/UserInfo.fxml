<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="userAnchorPane" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UserInfoController">
   <children>
      <ImageView fx:id="img" layoutX="50.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="un" alignment="TOP_LEFT" contentDisplay="TOP" layoutX="300.0" prefHeight="50.0" text="User name">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label fx:id="pd" alignment="TOP_LEFT" contentDisplay="TOP" layoutX="600.0" prefHeight="50.0" text="Password">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label fx:id="ia" alignment="TOP_LEFT" contentDisplay="TOP" layoutX="700.0" prefHeight="50.0" prefWidth="83.0" text="Not Admin">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button fx:id="buttonChange" layoutX="914.0" layoutY="6.0" mnemonicParsing="false" prefWidth="110.0" text="Change" />
      <Button fx:id="buttonDelete" layoutX="1033.0" layoutY="6.0" mnemonicParsing="false" prefWidth="110.0" text="Delete" />
      <Line endX="1200.0" AnchorPane.bottomAnchor="0.0" />
   </children>
</AnchorPane>
